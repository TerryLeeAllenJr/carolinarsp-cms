{
  "require": {
    "craftcms/cms": "^3.0.0",
    "craftcms/element-api": "2.5.4",
    "craftcms/mailgun": "1.4.2",
    "craftcms/redactor": "2.3.2",
    "ether/logs": "3.0.0",
    "studioespresso/craft-dumper": "1.3.1",
    "vaersaagod/dospaces": "1.1.1",
    "verbb/cp-nav": "2.0.9",
    "verbb/expanded-singles": "1.0.7",
    "vlucas/phpdotenv": "^2.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
